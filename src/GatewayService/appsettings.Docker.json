{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "IdentityServiceUrl": "http://identity",
  "ReverseProxy": {
    "Clusters": {
      "catalog": {
        "Destinations": {
          "catalogApi": {
            "Address": "http://catalog"
          }
        }
      },
      "search": {
        "Destinations": {
          "searchApi": {
            "Address": "http://search"
          }
        }
      },
      "notification": {
        "Destinations": {
          "notificationApi": {
            "Address": "http://notification"
          }
        }
      }
    }
  }
}
